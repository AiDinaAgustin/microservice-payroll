{
   "name": "hrpayroll360-api",
   "version": "1.0.0",
   "description": "Express API for HR Payroll 360 Website",
   "main": "src/index.ts",
   "scripts": {
      "start": "ts-node src/index.ts",
      "start:dist": "node dist/src/index.js",
      "dev": "nodemon src/index.ts",
      "build": "tsc",
      "lint": "eslint --fix src test",
      "test": "jest",
      "typecheck": "tsc --noEmit",
      "format": "prettier --write .",
      "migrate:generate": "npx sequelize-cli migration:generate --name",
      "migrate": "npx sequelize-cli db:migrate",
      "migrate:undo": "npx sequelize-cli db:migrate:undo",
      "seed": "npx sequelize-cli db:seed:all",
      "seed:undo": "npx sequelize-cli db:seed:undo"
   },
   "keywords": [],
   "author": "Keris Jateng Development Team",
   "repository": {
      "type": "git",
      "url": "https://gitlab.com/bigio/bi-jateng/kerisjateng/kerisjateng-api"
   },
   "license": "MIT",
   "dependencies": {
      "@faker-js/faker": "^9.2.0",
      "axios": "^1.7.7",
      "bcrypt": "^5.1.1",
      "cors": "^2.8.5",
      "date-fns": "^4.1.0",
      "dotenv": "^16.4.5",
      "exceljs": "^4.4.0",
      "express": "^4.21.1",
      "express-fileupload": "^1.5.1",
      "helmet": "^8.0.0",
      "http-status-codes": "^2.3.0",
      "jose": "^5.9.6",
      "module-alias": "^2.2.3",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "node-mocks-http": "^1.17.2",
      "pg": "^8.13.1",
      "pg-hstore": "^2.3.4",
      "read-excel-file": "^5.8.6",
      "redis": "^5.5.6",
      "sequelize": "^6.37.4",
      "swagger-ui-express": "^5.0.1",
      "url": "^0.11.4",
      "uuid": "^11.0.3",
      "webdav": "^5.7.1",
      "winston": "^3.17.0",
      "winston-daily-rotate-file": "^5.0.0",
      "yaml": "^2.6.0",
      "yup": "^1.4.0",
      "zod": "^3.25.20"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/express-fileupload": "^1.5.1",
      "@types/jest": "^29.5.13",
      "@types/morgan": "^1.9.9",
      "@types/multer": "^1.4.12",
      "@types/node": "^22.7.5",
      "@types/redis": "^4.0.10",
      "@types/supertest": "^6.0.2",
      "@types/swagger-ui-express": "^4.1.6",
      "@types/yup": "^0.29.14",
      "@typescript-eslint/eslint-plugin": "^7.16.1",
      "@typescript-eslint/parser": "^7.16.1",
      "eslint": "^8.57.0",
      "eslint-config-airbnb-typescript": "^18.0.0",
      "eslint-import-resolver-typescript": "^3.6.3",
      "eslint-plugin-import": "^2.31.0",
      "jest": "^29.7.0",
      "nodemon": "^3.1.7",
      "prettier": "^3.3.3",
      "sequelize-cli": "^6.6.2",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.5",
      "ts-node": "^10.9.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.6.3"
   },
   "_moduleAliases": {
      "@config": "src/config",
      "@controllers": "src/controllers",
      "@interfaces": "src/plugins/interfaces",
      "@constants": "src/plugins/constants",
      "@enums": "src/plugins/enums",
      "@middlewares": "src/middlewares",
      "@routes": "src/routes",
      "@models": "src/schemas/models",
      "@responses": "src/schemas/responses",
      "@validators": "src/schemas/validators",
      "@services": "src/services",
      "@utils": "src/utils"
   }
}
